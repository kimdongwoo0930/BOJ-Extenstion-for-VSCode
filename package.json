{
  "name": "boj-extension-for-vscode",
  "displayName": "BOJ Extension for VSCode - Beta",
  "description": "백준 온라인 문제를 VS Code에서 풀어보자.",
  "version": "0.0.7",
  "author": "Dong",
  "publisher": "Dong",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Your Extension Settings",
      "properties": {
        "BOJ-EX.GPT-API": {
          "type": "string",
          "default": "",
          "description": "GPT API키를 넣어주세요."
        }
      }
    },
    "commands": [
      {
        "command": "boj-extension-for-vscode.getProblemByNumber",
        "title": "BOJ: 문제 가져오기"
      },
      {
        "command": "boj-extension-for-vscode.checkTestCase",
        "title": "BOJ: 테스트"
      },
      {
        "command": "boj-extension-for-vscode.showProblem",
        "title": "BOJ: 문제 보기"
      },
      {
        "command": "boj-extension-for-vscode.gpt-hint",
        "title": "BOJ: 힌트 물어보기"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ignore-loader": "^0.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bufferutil": "^4.0.8",
    "canvas": "^2.11.2",
    "cheerio": "^1.0.0-rc.12",
    "child_process": "^1.0.2",
    "jsdom": "^24.1.0",
    "rss-parser": "^3.13.0",
    "utf-8-validate": "^6.0.4",
    "xmldom": "^0.6.0"
  }
}
